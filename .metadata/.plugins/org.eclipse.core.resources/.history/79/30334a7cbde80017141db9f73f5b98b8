		Map<Integer, List<Integer>> map = new LinkedHashMap<Integer, List<Integer>>();
		for(int i=0; i<a.length;i++) {
			if(!map.isEmpty()) {
				Iterator<Integer> iter = map.keySet().iterator();
			}
			if(!map.containsKey(a[i])) {
				List<Integer> list = new ArrayList<Integer>();
				list.a
			}
		}
	}

}
